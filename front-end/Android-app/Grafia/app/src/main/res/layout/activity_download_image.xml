<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".DownloadImageActivity">

    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@android:drawable/ic_menu_save"
            android:id="@+id/saveImage2"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="12dp" android:layout_marginStart="36dp"
            app:layout_constraintStart_toEndOf="@+id/BackToProject6"/>
    <ImageView
            android:layout_width="369dp"
            android:layout_height="438dp" app:srcCompat="@color/colorPrimary"
            android:id="@+id/imageView2"
            android:layout_marginTop="5dp"
            app:layout_constraintTop_toBottomOf="@+id/BackToProject6" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="5dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="10dp"
            android:layout_marginBottom="4dp" app:layout_constraintBottom_toBottomOf="parent"/>
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/BackToProject6" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="16dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="280dp"/>
</android.support.constraint.ConstraintLayout>