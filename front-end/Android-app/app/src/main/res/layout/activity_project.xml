<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ProjectActivity">

    <Button
            android:text="Add Participant"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="16dp"
            tools:layout_editor_absoluteX="250dp" android:id="@+id/button3"/>
    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@android:drawable/ic_menu_camera"
            tools:layout_editor_absoluteY="64dp" tools:layout_editor_absoluteX="16dp" android:id="@+id/imageButton"/>
    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@android:drawable/ic_menu_gallery"
            tools:layout_editor_absoluteY="64dp" tools:layout_editor_absoluteX="88dp" android:id="@+id/imageButton2"/>
    <Button
            android:text="Save?"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="68dp"
            tools:layout_editor_absoluteX="282dp" android:id="@+id/button5"/>
</android.support.constraint.ConstraintLayout>